{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "component-commands": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "copying": {
      "type": "object",
      "properties": {
        "authors": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "license-name": {
          "type": "string"
        },
        "license-url": {
          "type": "string"
        },
        "year": {
          "type": "integer"
        }
      },
      "additionalProperties": false
    },
    "description": {
      "type": "object",
      "properties": {
        "sha1-hex": {
          "type": "string"
        },
        "string": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "invocations": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "sha1-hex": {
            "type": "string"
          },
          "invocation-string": {
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": [
          "invocation-string"
        ]
      }
    },
    "uuid": {
      "description": "Universally Unique Identifier for this CmdOyster.",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "component-commands",
    "description",
    "invocations",
    "uuid"
  ]
}
