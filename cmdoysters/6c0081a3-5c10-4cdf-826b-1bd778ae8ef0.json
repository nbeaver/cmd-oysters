    "description": {
        "string": "For each file ending with '.pdf' in the current directory, extract the images from the pdf file into a new directory with the name of the file stripped of '.pdf'."
    },
    "invocations": [
        {
            "changeable-arguments": {
                "\"${f%.*}\"": {
                    "description-string": "Filename of PDF with '.pdf' stripped off. Used as the directory to store the images and image-root for image filenames.",
                    "invocation-slice": [
                        23,
                        32
                    ],
                    "subtype": "Shell expression that can be stored in a variable.",
                    "type": "string"
                }
            },
            "invocation-string": "for f in *.pdf; do dir=\"${f%.*}\"; mkdir -p \"$dir\" && pdfimages -png -j \"$f\" \"$dir/$dir\"; done",
