{
    "can-modify": {
        "filesystem": true,
        "other-processes": false
    },
    "component-commands": [
        "gzip"
    ],
    "copying": {
        "authors": [
            "Nathaniel Morck Beaver"
        ],
        "license-name": "MIT (Expat) License",
        "license-url": "http://opensource.org/licenses/MIT",
        "year": 2015
    },
    "description": {
        "string": "Compress a file using gzip without removing the original file."
    },
    "invocations": [
        {
            "changeable-arguments": {
                "file": {
                    "description-string": "Path to file to be compressed.",
                    "invocation-slice": [
                        7,
                        11
                    ],
                    "subtype": "File path.",
                    "type": "string"
                },
                "file.gz": {
                    "description-string": "Path to output compressed file.",
                    "invocation-slice": [
                        14,
                        21
                    ],
                    "subtype": "File path.",
                    "type": "string"
                }
            },
            "comment": "Use gzip as a filter.",
            "string": "gzip < file > file.gz"
        },
        {
            "command-requirements-as-invoked": {
                "gzip": {
                    "version-specifier": "1.6 or later"
                }
            },
            "comment": "Built-in feature of recent versions of gzip (short flag).",
            "string": "gzip -k file"
        },
        {
            "command-requirements-as-invoked": {
                "gzip": {
                    "version-specifier": "1.6 or later"
                }
            },
            "comment": "Built-in feature of recent versions of gzip (long flag).",
            "string": "gzip --keep file"
        }
    ],
