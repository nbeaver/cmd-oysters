    "description": {
        "verbose-description": "Show the date and time in a different timezone without changing the system time zone."
    },
    "invocations": [
        {
            "changeable-arguments": {
                "America/Argentina/Buenos_Aires": {
                    "argument-description": "Any timezone in the tz database.",
                    "invocation-slice": [
                        3,
                        33
                    ],
                    "subtype": "Timezone code.",
                    "type": "string"
                }
            },
            "comment": "Date and time in Buenos Aires, Argentina.",
            "example-outputs": [
                "Thu Jan  1 00:00:00 ART 1970"
            ],
            "invocation-string": "TZ=America/Argentina/Buenos_Aires date",
