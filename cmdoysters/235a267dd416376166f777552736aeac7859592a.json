    "description": {
        "sha1-hex": "235a267dd416376166f777552736aeac7859592a",
        "string": "Find all directory paths matching '*doc*' in `/usr/share`, except the paths under `/usr/share/doc`. This can help find documentation that is in an unusual place."
    },
    "invocations": [
        {
            "changeable-arguments": {
                "*doc*": {
                    "component-command": "find",
                    "component-command-flag": "-name",
                    "description-string": "Pattern to match the path to.",
                    "invocation-slice": [
                        67,
                        72
                    ],
                    "subtype": "Relative or absolute path.",
                    "type": "string"
                },
                "/usr/share/": {
                    "component-command": "find",
                    "description-string": "Path to search under.",
                    "invocation-slice": [
                        6,
                        17
                    ],
                    "subtype": "Relative or absolute path.",
                    "type": "string"
                },
                "/usr/share/doc": {
                    "component-command": "find",
                    "component-command-flag": "-path",
                    "description-string": "Path to exclude (prune) from search.",
                    "invocation-slice": [
                        26,
                        40
                    ],
                    "subtype": "Relative or absolute path.",
                    "type": "string"
                }
            },
            "sha1-hex": "6ed941ee9fd2cbd099d1cdfd1aed1cb4ac65e498",
            "string": "find '/usr/share/' -path '/usr/share/doc' -prune -o -type d -name '*doc*'"
        }
    ],
